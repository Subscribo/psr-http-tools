{
    "name": "subscribo/psr-http-tools",
    "description": "Tools which could be used with PSR-7: Request factory, Uri factory, Response parser",
    "license": "MIT",
    "keywords": [
        "PSR-7",
        "factory",
        "parser",
        "request",
        "response",
        "uri"
    ],
    "homepage": "http://www.subscribo.io",
    "authors": [
        {
            "name": "Slavo",
            "role": "developer"
        },
        {
            "name": "Kochabo",
            "role": "owner"
        }
    ],
    "support": {
        "email": "packages@subscribo.io",
        "issues": "https://github.com/Subscribo/psr-http-tools/issues",
        "source": "https://github.com/Subscribo/psr-http-tools"
    },
    "require": {
        "php": ">=5.4",
        "psr/http-message": "^0.11.0",
        "phly/http": "^0.13.0"
    },
    "require-dev": {
        "phpunit/phpunit": "<5.0"
    },
    "autoload": {
        "psr-4": {
            "Subscribo\\PsrHttpTools\\": "src/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "0.2.x-dev"
        }
    }
}
